<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.arunkarka.nexuslexic2" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TeacherActivity"
            android:label="@string/title_activity_teacher" >
        </activity>
        <activity
            android:name=".StudentActivity"
            android:label="@string/title_activity_student" >
        </activity>
        <activity
            android:name=".StudentLikesActivity"
            android:label="@string/title_activity_student_likes" >
        </activity>
        <activity
            android:name=".AssignmentOptionsActivity"
            android:label="@string/title_activity_assignment_options" >
        </activity>
        <activity
            android:name=".StudentListActivity"
            android:label="@string/title_activity_student_list" >
        </activity>
    </application>

</manifest>
